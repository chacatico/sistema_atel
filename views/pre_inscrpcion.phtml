<center>
<form name="pre_inscripcion" method="post" action="">
	<table>
		<tr align="center">
			<td colspan="2">
				<h3>Datos Personales</h3>
			</td>
		</tr>
		<tr>
			<td>
				Cédula: 
			</td>
			<td>
				<select class="input-mini" name="nacionalidad" id="nacionalidad">
					<option value="0">..</option>
					<option value="1">V</option>
					<option value="2">E</option>
				</select>
				<input type="text" name="cedula" id="cedula" placeholder="12345678" class="input-small">
			</td>
		</tr>
		<tr>
			<td>
				Nombres: 
			</td>
			<td>
				<input type="text" name="nombre" id="nombre" placeholder="Nombre Completo" class="input-medium">
			</td>
		</tr>
		<tr>
			<td>
				Apellidos: 
			</td>
			<td>
				<input type="text" name="apellido" id="apellido" placeholder="Apellido Completo" class="input-medium">
			</td>
		</tr>
		<tr>
			<td>
				Correo: 
			</td>
			<td>
				<input type="text" name="correo" id="correo" placeholder="ejemplo@gmail.com" class="input-medium">
			</td>
		</tr>
		<tr>
			<td>
				Profesión:
			</td>
			<td>
				<select class="input-medium" name="profesion" id="profesion">
					<option value="0">Seleccione</option>
					<option value="1">Estudiante</option>
					<option value="2">Profesional</option>
				</select>
			</td>
		</tr>
		<tr align="center">
			<td colspan="2">
				<input type="submit" value="Enviar" class="btn btn-primary">
				<input type="reset" value="Limpiar" class="btn btn-primary">
				<!-- <input type="hidden"id="grupo" name="grupo" value="2"/> -->
				<input type="hidden" name="grabar" value="si"/>
			</td>
		</tr>
	</table>
</form>
</center>